@model string
<div class="jumbotron" style="background-image: url(@Model);" >
    <div class="overlay"></div>
    <div class="container">
        <div class="row">
            <div class="col-lg-8 col-md-10 mx-auto">
                <div class="site-heading">
                    <h1>@Sitecore.Globalization.Translate.Text("Hero banner heading")</h1>
                    <span class="subheading">@Sitecore.Globalization.Translate.Text("Hero banner sub heading")</span>
                </div>
            </div>
        </div>
    </div>
</div>